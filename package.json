    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "mqtt": "^5.14.1",
    "msgpackr": "^1.11.5",
    "node-fetch": "^3.3.2",
    "redis": "^5.9.0",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "tar-fs": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "unzipper": "^0.12.3",
    "uuid": "^13.0.0"
